<!DOCTYPE html>
<html lang="en">

<%- include('./common/head') %>

<body>
    <div class="container-xxl bg-white p-0">
        <!-- Spinner Start -->
        <div id="spinner" class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
            <div class="spinner-border text-primary" style="width: 3rem; height: 3rem;" role="status">
                <span class="sr-only">Loading...</span>
            </div>
        </div>
        <!-- Spinner End -->


        <!-- Navbar Start -->
        <%- include('./common/nav') %>
        <!-- Navbar End -->


        <!-- Header End -->
        <div class="container-xxl py-5 bg-dark page-header mb-5">
            <div class="container my-5 pt-5 pb-4">
                <h1 class="display-3 text-white mb-3 animated slideInDown">About Us</h1>
                <nav aria-label="breadcrumb">
                   
                </nav>
            </div>
        </div>
        <!-- Header End -->


        <!-- About Start -->
        <% for(let data of aboutdata ){ %>
        <div class="container-xxl py-5">
            <div class="container">
                <div class="row g-5 align-items-center">
                    <div class="col-lg-6 wow fadeIn" data-wow-delay="0.1s">
                        <div class="row g-0 about-bg rounded overflow-hidden">
                            <div class="col-6 text-start">
                                <img class="img-fluid w-100" src="<%= data.imageone %>">
                            </div>
                            <div class="col-6 text-start">
                                <img class="img-fluid" src="<%= data.imagetwo %>" style="width: 85%; margin-top: 15%;">
                            </div>
                            <div class="col-6 text-end">
                                <img class="img-fluid" src="<%= data.imagethree %>" style="width: 85%;">
                            </div>
                            <div class="col-6 text-end">
                                <img class="img-fluid w-100" src="<%= data.imagefour %>">
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6 wow fadeIn" data-wow-delay="0.5s">
                        <h1 class="mb-4"><%= data.title %></h1>
                        <p class="mb-4"><%= data.subtitle %></p>
                        <p><i class="fa fa-check text-primary me-3"></i><%= data.para1 %></p>
                        <p><i class="fa fa-check text-primary me-3"></i><%= data.para2 %></p>
                        <p><i class="fa fa-check text-primary me-3"></i><%= data.para3 %></p>
                        <a class="btn btn-primary py-3 px-5 mt-3" href="/">Read More</a>
                    </div>
                </div>
            </div>
        </div>
        <% } %>
        <!-- About End -->


        <!-- Footer Start -->
       <%- include('./common/footer') %>
</body>

</html>