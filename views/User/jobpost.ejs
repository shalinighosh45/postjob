  <body>
    <section class="header">
      <div class="logo">
        <i class="ri-menu-line icon icon-0 menu"></i>
        <h2>JobNest<span>Hub</span></h2>
      </div>
      <div class="search--notification--profile">
        <div class="search">
          <input type="text" placeholder="Search Scdule.." />
          <button><i class="ri-search-2-line"></i></button>
        </div>
        <div class="notification--profile">
          <div class="picon lock">
            <i class="ri-lock-line"></i>
          </div>
          <br />

          <div class="picon bell">
            <i class="ri-notification-2-line"></i>
          </div>
          <div class="picon chat">
            <i class="ri-wechat-2-line"></i>
          </div>
        </div>
      </div>
    </section>
    <section class="main">
      <div class="sidebar">
        <ul class="sidebar--items">
          <li>
            <div class="img--box1">
              <img
                src="https://source.unsplash.com/beautiful-young-woman-looking-at-camera-and-smiling-while-standing-against-grey-background-rZC98_IKxCU"
                alt=""
              />
            </div>
            <h2 style="margin-left: 80px"><%= data.name %></h2>
          </li>
          <br />
          <li>
            <a href="#" id="active--link">
              <span class="icon icon-1"
                ><i class="ri-layout-grid-line"></i
              ></span>
              <span class="sidebar--item">Dashboard</span>
            </a>
          </li>
          <li>
            <a href="#">
              <span class="icon icon-2"
                ><i class="ri-calendar-2-line"></i
              ></span>
              <span class="sidebar--item">Personal-details</span>
            </a>
          </li>
          <li>
            <a href="#">
              <span class="icon icon-3"><i class="ri-user-2-line"></i></span>
              <span class="sidebar--item" style="white-space: nowrap"
                >Academic</span
              >
            </a>
          </li>
          <li>
            <a href="#">
              <span class="icon icon-4"><i class="ri-user-line"></i></span>
              <span class="sidebar--item">Experience</span>
            </a>
          </li>
          <li>
            <a href="#">
              <span class="icon icon-5"
                ><i class="ri-line-chart-line"></i
              ></span>
              <span class="sidebar--item">Activity</span>
            </a>
          </li>
          <li>
            <a href="#">
              <span class="icon icon-6"
                ><i class="ri-customer-service-line"></i
              ></span>
              <span class="sidebar--item">Support</span>
            </a>
          </li>
        </ul>
        <ul class="sidebar--bottom-items">
          <li>
            <a href="/">
              <span class="icon icon-8"
                ><i class="ri-logout-box-r-line"></i
              ></span>
              <span class="sidebar--item">Home page</span>
            </a>
          </li>
          <li>
            <a href="/userlogin">
              <span class="icon icon-8"
                ><i class="ri-logout-box-r-line"></i
              ></span>
              <span class="sidebar--item">Logout</span>
            </a>
          </li>
        </ul>
      </div>
      <div class="main--content">
        <div class="overview"></div>

        <div class="recent--patients">
          <div class="title">
            <h1 class="section--title" style="color: #1f39ff">Post a job</h1>
            <br />
          </div>
        </div>

        <form action="/create/job" method="post" enctype="multipart/form-data" >
          <!-- Company Image URL -->
          <label for="companyImage">Company Image URL:</label>
          <input type="file" id="companyImage" name="companyimage" required />

          <!-- Job Title -->
          <label for="jobTitle">Job Title:</label>
          <input type="text" id="jobTitle" name="jobTitle" required />

          <!-- Location -->
          <label for="location">Location:</label>
          <input type="text" id="location" name="location" required />

          <!-- Employment Type -->
          <label for="employmentType">Employment Type:</label>
          <input
            type="text"
            id="employmentType"
            name="employmentType"
            required
          />

          <!-- Salary -->
          <label for="salary">Salary:</label>
          <input type="text" id="salary" name="salary" required />

          <!-- Job Description -->
          <label for="jobDescription">Job Description:</label>
          <textarea
            id="jobDescription"
            name="jobDescription"
            rows="4"
            required
          ></textarea>

          <!-- Responsibilities (comma-separated) -->
          <label for="responsibilities"
            >Responsibilities (comma-separated):</label
          >
          <input
            type="text"
            id="responsibilities"
            name="responsibilities"
            required
          />

          <!-- Qualifications (comma-separated) -->
          <label for="qualifications">Qualifications (comma-separated):</label>
          <input
            type="text"
            id="qualifications"
            name="qualifications"
            required
          />

          <!-- Published On -->
          <label for="publishedOn">Published On:</label>
          <input type="date" id="publishedOn" name="publishedOn" required />

          <!-- Vacancy -->
          <label for="vacancy">Vacancy:</label>
          <input type="number" id="vacancy" name="vacancy" required />

          <!-- Job Nature -->
          <label for="jobNature">Job Nature:</label>
          <input type="text" id="jobNature" name="jobNature" required />

          <!-- Deadline -->
          <label for="deadline">Deadline:</label>
          <input type="date" id="deadline" name="deadline" required />

          <label for="companyName">CompanyName:</label>
          <input type="text" id="companyName" name="companyName" required />

          <label for="companyDetails">CompanyDetails:</label>
          <input type="text" id="companyDetails" name="companyDetails" required />

          <!-- Submit Button -->
          <button type="submit">Submit Job Posting</button>
        </form>
      </div>
    </section>
    <script src="assets/js/main.js"></script>
